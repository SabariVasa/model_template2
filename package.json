{
  "name": "catering",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^5.1.3",
    "formik": "^2.2.9",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.1.1",
    "react": "^16.13.1",
    "react-datepicker": "^3.8.0",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "react-semantic-toasts": "^0.6.5",
    "react-semantic-ui-datepickers": "^2.6.0",
    "react-slick": "^0.26.1",
    "semantic-ui-react": "^0.88.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "NODE_OPTIONS=--openssl-legacy-provider craco start",
    "build": "NODE_OPTIONS=--openssl-legacy-provider GENERATE_SOURCEMAP=true craco --max_old_space_size=8192 build",
    "test": "NODE_OPTIONS=--openssl-legacy-provider craco test",
    "eject": "NODE_OPTIONS=--openssl-legacy-provider craco eject",
    "lint": "eslint . --ext js,jsx",
    "release": "standard-version",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
},
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.24.6",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@craco/craco": "^5.6.4",
    "@semantic-ui-react/craco-less": "^1.2.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "babel-loader": "^8.4.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "craco-less": "1.16.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^4.2.5",
    "less-loader": "^6.1.1",
    "semantic-ui-less": "^2.4.1",
    "source-map-explorer": "^2.5.2",
    "standard-version": "^8.0.1",
    "style-loader": "^1.2.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
